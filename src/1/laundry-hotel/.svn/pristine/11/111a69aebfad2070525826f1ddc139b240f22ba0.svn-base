<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="FuncWindow" TargetType="Window">
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Width" Value="1024"/>
        <Setter Property="Height" Value="768"/>
        <Setter Property="WindowState" Value="Normal"></Setter>
        <Setter Property="ResizeMode" Value="CanMinimize"></Setter>
        <Setter Property="WindowStyle" Value="None"></Setter>
        <Setter Property="Background" Value="{DynamicResource MainWinBg}"></Setter>
        <Setter Property="Icon" Value="../../icon.ico"></Setter>
    </Style>
    <ImageBrush x:Key="FuncWinBg" ImageSource="Images\bg-neiye1.png"></ImageBrush>
    <ImageBrush x:Key="MainWinBg" ImageSource="Images\bg_shouye_01.png"></ImageBrush>
    <BitmapImage x:Key="MainLogo" UriSource="Images\logo-neiye.png"></BitmapImage>

    <ImageBrush x:Key="HomeTheme1" ImageSource="Images\bg_shouye_01.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme2" ImageSource="Images\bg_shouye_02.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme3" ImageSource="Images\bg_shouye_03.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme4" ImageSource="Images\bg_shouye_04.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme5" ImageSource="Images\bg_shouye_05.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme6" ImageSource="Images\bg_shouye_06.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme7" ImageSource="Images\bg_shouye_07.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme8" ImageSource="Images\bg_shouye_08.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme9" ImageSource="Images\bg_shouye_09.png"></ImageBrush>
    <ImageBrush x:Key="HomeTheme10" ImageSource="Images\bg_shouye_10.png"></ImageBrush>

    <ImageBrush x:Key="InPageTheme1" ImageSource="Images\bg-neiye1.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme2" ImageSource="Images\bg-neiye2.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme3" ImageSource="Images\bg-neiye3.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme4" ImageSource="Images\bg-neiye4.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme5" ImageSource="Images\bg-neiye5.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme6" ImageSource="Images\bg-neiye6.png"></ImageBrush>
    <ImageBrush x:Key="InPageTheme7" ImageSource="Images\bg-neiye7.png"></ImageBrush> 
</ResourceDictionary>