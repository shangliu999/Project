
@{
    Layout = null;
}
<div class="modal inmodal" id="DetailModel" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">投诉</h4>    
            </div>
            <form class="form-horizontal" id="CustomerService">
                <div style="margin:0 auto;width:400px;height:500px;">
                    <textarea id="PublishMessage" class="col-sm-8" style="height:500px;width:400px;margin:auto"></textarea>
                </div>
                <div class="modal-footer" >
                    <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                    <button type="submit" class="btn btn-primary">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>

