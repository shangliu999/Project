
@{
    Layout = null;
}
<div class="modal inmodal" id="DetailModel" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="DetailTitle"></h4>
            </div>
            <form class="form-horizontal">
                <div class="modal-body">
                    <div class="form-group">
                        <label class="col-lg-2 control-label">单据号：</label>
                        <div class="col-lg-4">
                            <label id="InvNoId" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">制单人：</label>
                        <div class="col-lg-4">
                            <label id="CreatUser" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">时间：</label>
                        <div class="col-lg-4">
                            <label id="CreatTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">酒店：</label>
                        <div class="col-lg-4">
                            <label id="Hotel" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">楼层：</label>
                        <div class="col-lg-4">
                            <label id="Floor" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div><div class="form-group">
                        <label class="col-lg-2 control-label">确认人：</label>
                        <div class="col-lg-4">
                            <label id="ConfirmUserName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">确认时间：</label>
                        <div class="col-lg-4">
                            <label id="ConfirmTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="table-responsive dataTables_wrapper" style="margin-left:20px;margin-right:20px;">
                            <table id="select-table-detail" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>类型</th>
                                        <th>尺寸</th>
                                        <th>数量</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div>
    </div>
</div>

