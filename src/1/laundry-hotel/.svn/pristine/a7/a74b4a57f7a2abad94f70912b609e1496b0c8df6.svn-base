<Window x:Class="ETexsys.WashingCabinet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETexsys.WashingCabinet"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" WindowStyle="None"
        Title="MainWindow" Height="600" Width="800" Activated="Window_Activated">
    <Grid Background="{StaticResource bg}">
        <Grid.RowDefinitions>
            <RowDefinition Height="70px"></RowDefinition>
            <RowDefinition Height="158px"></RowDefinition>
            <RowDefinition/>
            <RowDefinition Height="28"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="{StaticResource bgtable}">
            <Image Source="{StaticResource logo}" Width="36px" Height="34px" Margin="25,0,0,0" HorizontalAlignment="Left"></Image>
            <Label Content="内科-科室仓库" Foreground="#FFFFFF" FontSize="20px" Margin="73,19,2,6"></Label>
            <Label Name="lbTime" Foreground="#21AF37" Content="" FontSize="20px" HorizontalAlignment="Right" Margin="0,19,25,0"></Label>

            <Ellipse Width="50" Height="50" Stroke="#ff71a0" Canvas.Left="19" Canvas.Top="36" Margin="0,0,70,0"  />
            <Ellipse Width="50" Height="50" Stroke="#21AF37" Canvas.Left="186" Margin="90,0,0,0" />
            <Ellipse Width="50" Height="50" Stroke="#0294DD" Canvas.Left="285" Canvas.Top="10" Margin="250,0,0,0"  />
            <Ellipse Width="50" Height="50" Stroke="#FA8305" Canvas.Left="84" Canvas.Top="6" Margin="0,0,230,0"/>
            <Canvas>
                <Label Foreground="#FA8305" Content="70℃" FontSize="12px" Canvas.Left="267" Canvas.Top="16"></Label>
                <Label Foreground="#FFFFFF" Content="温度" FontSize="10px" Canvas.Left="270" Canvas.Top="32"></Label>
            </Canvas>
            <Canvas>
                <Label Foreground="#ff71a0" Content="5℃" FontSize="12px" Canvas.Left="351" Canvas.Top="16"></Label>
                <Label Foreground="#FFFFFF" Content="湿度" FontSize="10px" Canvas.Left="350" Canvas.Top="32"></Label>
            </Canvas>
            <Canvas>
                <Label Foreground="#21AF37" Content="真菌" FontSize="12px" Canvas.Left="428" Canvas.Top="16"></Label>
                <Label Foreground="#FFFFFF" Content="灭菌" FontSize="10px" Canvas.Left="430" Canvas.Top="32"></Label>
            </Canvas>
            <Canvas>
                <Label Foreground="#0294DD" Content="101KPa" FontSize="12px" Canvas.Left="499" Canvas.Top="16"></Label>
                <Label Foreground="#FFFFFF" Content="气压" FontSize="10px" Canvas.Left="510" Canvas.Top="32"></Label>
            </Canvas>
        </Grid>
        <Grid Grid.Row="1" Margin="0,14,0,0" Height="144" VerticalAlignment="Top" RenderTransformOrigin="0.499,0.5">
            <StackPanel Height="144" Background="#292E42" Opacity="0.7" Width="760"/>
            <DataGrid Style="{DynamicResource DefaultGrid}" Width="760" Height="145" ItemsSource="{Binding}" DataContext="{Binding Model.MessageList}">
                <DataGrid.RowStyle>
                    <Style TargetType="{x:Type DataGridRow}">
                        <Style.Triggers>
                            <Trigger Property="ItemsControl.AlternationIndex"  Value="0">
                                <Setter Property="Background" Value="Transparent" />
                            </Trigger>
                            <Trigger Property="ItemsControl.AlternationIndex" Value="1">
                                <Setter Property="Background" Value="#F0F7FF" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </DataGrid.RowStyle>
                <DataGrid.ColumnHeaderStyle>
                    <Style TargetType="{x:Type DataGridColumnHeader}">
                        <Setter Property="Background" Value="#292E42"/>
                        <Setter Property="Foreground" Value="#FFFFFF" />
                        <Setter Property="Height" Value="36" />
                        <Setter Property="FontFamily" Value="Microsoft YaHei"></Setter>
                        <Setter Property="FontSize" Value="18"></Setter>
                    </Style>
                </DataGrid.ColumnHeaderStyle>
                <DataGrid.Columns>
                    <DataGridTemplateColumn Width="60">
                        <DataGridTemplateColumn.Header>
                            <Grid>
                                <Label Content="时间" Margin="15,0,0,0" HorizontalAlignment="Center" FontSize="18" Foreground="#FFFFFF"  ></Label>
                            </Grid>
                        </DataGridTemplateColumn.Header>
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <TextBlock  Text="{Binding Path=Time}" Margin="15,0,0,0" FontFamily="微软雅黑" FontSize="18" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>
                    <DataGridTemplateColumn Width="147">
                        <DataGridTemplateColumn.Header>
                            <TextBlock Text="姓名" FontSize="18" Margin="60,0,0,0"></TextBlock>
                        </DataGridTemplateColumn.Header>
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <TextBlock Foreground="#FFFFFF" Text="{Binding Path=OperatorName}" Margin="15,0,0,0" FontFamily="微软雅黑" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>
                    <DataGridTemplateColumn Width="65">
                        <DataGridTemplateColumn.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="状态" Margin="14,0,0,0"></TextBlock>
                            </StackPanel>
                        </DataGridTemplateColumn.Header>
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel>
                                    <Grid>
                                        <TextBlock Visibility="{Binding DeliveryVisible}" Foreground="#FA8305" Text="投放"  FontFamily="微软雅黑" FontSize="18" Margin="0,5,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        <TextBlock Visibility="{Binding ReceiveVisible}" Foreground="#21AF37" Text="领取"  FontFamily="微软雅黑" FontSize="18" Margin="0,5,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    </Grid>
                                </StackPanel>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>
                    <DataGridTemplateColumn Width="*">
                        <DataGridTemplateColumn.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="详情" Margin="190,0,0,0"></TextBlock>
                            </StackPanel>
                        </DataGridTemplateColumn.Header>
                        <DataGridTemplateColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel>
                                    <ListBox Width="400" ItemsSource="{Binding}" DataContext="{Binding MessageDetailsList}" HorizontalAlignment="Left" Background="Transparent" BorderThickness="0" Margin="20,4,0,0">
                                        <ListBox.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <UniformGrid Columns="5"></UniformGrid>
                                            </ItemsPanelTemplate>
                                        </ListBox.ItemsPanel>
                                        <ListBox.ItemTemplate>
                                            <DataTemplate>
                                                <StackPanel HorizontalAlignment="Left"  >
                                                    <Grid Width="75">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition/>
                                                            <ColumnDefinition Width="8"></ColumnDefinition>
                                                            <ColumnDefinition Width="32"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Grid.Column="0" HorizontalAlignment="Right"  Text="{Binding ClassName}" FontSize="18" Foreground="#FFFFFF"    />
                                                        <TextBlock Grid.Column="1" HorizontalAlignment="Left"  Text="-" FontSize="18" Foreground="#FFFFFF"    />
                                                        <TextBlock Grid.Column="2" HorizontalAlignment="Left" Text="{Binding ClassCount}" FontSize="18"  Foreground="#21AF37"   />
                                                    </Grid>
                                                </StackPanel>
                                            </DataTemplate>
                                        </ListBox.ItemTemplate>
                                        <ListBox.ItemContainerStyle>
                                            <Style TargetType="ListBoxItem">
                                                <!-- 设置控件模板 -->
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="ListBoxItem">
                                                            <Border Background="{TemplateBinding Background}">
                                                                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                                            </Border>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </ListBox.ItemContainerStyle>
                                    </ListBox>
                                </StackPanel>
                            </DataTemplate>
                        </DataGridTemplateColumn.CellTemplate>
                    </DataGridTemplateColumn>
                </DataGrid.Columns>
                <DataGrid.CellStyle>
                    <Style TargetType="DataGridCell">
                        <Setter Property="BorderThickness" Value="0"/>
                        <Style.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter Property="Background" Value="LightBlue"/>
                                <Setter Property="Foreground" Value="White"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </DataGrid.CellStyle>
            </DataGrid>
        </Grid>
        <Grid Grid.Row="2" Margin="0,16,0,0" VerticalAlignment="Top" Height="328px">
            <Label Visibility="{Binding Model.NetVisible}" Content="当前净物仓-存储量" FontSize="20px"  Foreground="#21AF37" Margin="17,2,0,0"></Label>
            <Button Visibility="{Binding Model.NetVisible}" Width="210px" Height="44px" VerticalAlignment="Top" Margin="90,0,0,0" Cursor="Hand" Command="{Binding GetDirt}">
                <Button.Template>
                    <ControlTemplate>
                        <StackPanel Background="{StaticResource wuwu}">
                            <Grid Margin="0,4,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="29*"></ColumnDefinition>
                                    <ColumnDefinition Width="76*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" Source="{StaticResource qiehuan}"  Width="20" HorizontalAlignment="Right" Margin="0,9"  />
                                <Label Grid.Column="1" Content="切换到污物仓" FontSize="18px" Foreground="#FFFFFF" Margin="10,0,0,0"></Label>
                            </Grid>
                        </StackPanel>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Label Content="操作提示" FontSize="20" Foreground="#666666" Margin="630,0,0,0"></Label>
            <StackPanel Visibility="{Binding Model.NetVisible}" Height="290" Width="530"  VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,50,0,0">
                <ListBox  Width="530px" ItemsSource="{Binding}" DataContext="{Binding Model.DetailsList}" Background="Transparent" BorderThickness="0">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <UniformGrid Columns="4"></UniformGrid>
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Width="130px" >
                                <Grid>
                                    <Grid Visibility="{Binding Path=Visible1}" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0" Width="130px" Height="42px" Background="#21AF37">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="{Binding ClassName}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Foreground="#FFFFFF" ></Label>
                                                <Label Grid.Column="1" Content="{Binding ClassCount}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="#FFFFFF" ></Label>
                                            </Grid>
                                        </StackPanel>
                                        <Grid Background="#FFFFFF"  Grid.Row="1" Width="130px" Height="98px">
                                            <ListBox Width="130" ItemsSource="{Binding}" DataContext="{Binding ChooesList}" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Center" BorderThickness="0">
                                                <ListBox.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <UniformGrid Columns="1"></UniformGrid>
                                                    </ItemsPanelTemplate>
                                                </ListBox.ItemsPanel>
                                                <ListBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Background="#FFFFFF">
                                                            <Grid Width="128">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70px"></ColumnDefinition>
                                                                    <ColumnDefinition Width="20px"></ColumnDefinition>
                                                                    <ColumnDefinition Width="38px"></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Label Margin="0,0,0,0" Grid.Column="0" HorizontalAlignment="Right" Content="{Binding SizeName}" FontSize="17" Foreground="#666666"/>
                                                                <Label Grid.Column="1" HorizontalAlignment="Left" Content="-" FontSize="17" Foreground="#666666" />
                                                                <Label Grid.Column="2" HorizontalAlignment="Left" Content="{Binding SizeCount}" FontSize="17" Foreground="#21AF37"/>
                                                            </Grid>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                                <ListBox.ItemContainerStyle>
                                                    <Style TargetType="ListBoxItem">
                                                        <!-- 设置控件模板 -->
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="ListBoxItem">
                                                                    <Border Background="{TemplateBinding Background}">
                                                                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                                                    </Border>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </ListBox.ItemContainerStyle>
                                            </ListBox>
                                        </Grid>
                                    </Grid>
                                    <Grid Visibility="{Binding Path=Visible2}" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0" Width="130px" Height="42px" Background="#21AF37">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="{Binding ClassName}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Foreground="#FFFFFF" ></Label>
                                                <Label Grid.Column="1" Content="{Binding ClassCount}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="#FFFFFF" ></Label>
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel  Grid.Row="1" Width="130px" Height="98px" Background="#FFFFFF">
                                            <Label Content="均码" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,30,0,0" FontSize="22" Foreground="#666666"></Label>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </StackPanel>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="ListBoxItem">
                            <!-- 设置控件模板 -->
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border Background="{TemplateBinding Background}">
                                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>
            </StackPanel>

            <Label Visibility="{Binding Model.DirtVisible}" Content="当前污物仓-投污量" FontSize="20px"  Foreground="#2167AF" Margin="17,2,0,0"></Label>
            <Button Visibility="{Binding Model.DirtVisible}" Width="210px" Height="44px" VerticalAlignment="Top" Margin="90,0,0,0" Cursor="Hand" Command="{Binding GoNet}">
                <Button.Template>
                    <ControlTemplate>
                        <StackPanel Background="{StaticResource jinwu}">
                            <Grid Margin="0,4,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="29*"></ColumnDefinition>
                                    <ColumnDefinition Width="76*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" Source="{StaticResource qiehuan}"  Width="20" HorizontalAlignment="Right" Margin="0,9"  />
                                <Label Grid.Column="1" Content="切换到净物仓" FontSize="18px" Foreground="#FFFFFF" Margin="10,0,0,0"></Label>
                            </Grid>
                        </StackPanel>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <StackPanel Visibility="{Binding Model.DirtVisible}" Height="290" Width="530"  VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,50,0,0">
                <ListBox  Width="530px" ItemsSource="{Binding}" DataContext="{Binding Model.DetailsList}" Background="Transparent" BorderThickness="0">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <UniformGrid Columns="4"></UniformGrid>
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Width="130px" >
                                <Grid>
                                    <Grid Visibility="{Binding Path=Visible1}" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0" Width="130px" Height="42px" Background="#2167AF">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="{Binding ClassName}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Foreground="#FFFFFF" ></Label>
                                                <Label Grid.Column="1" Content="{Binding ClassCount}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="#FFFFFF" ></Label>
                                            </Grid>
                                        </StackPanel>
                                        <Grid Background="#FFFFFF"  Grid.Row="1" Width="130px" Height="98px">
                                            <ListBox Width="130" ItemsSource="{Binding}" DataContext="{Binding ChooesList}" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Center" BorderThickness="0">
                                                <ListBox.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <UniformGrid Columns="1"></UniformGrid>
                                                    </ItemsPanelTemplate>
                                                </ListBox.ItemsPanel>
                                                <ListBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Background="#FFFFFF">
                                                            <Grid Width="128">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70px"></ColumnDefinition>
                                                                    <ColumnDefinition Width="20px"></ColumnDefinition>
                                                                    <ColumnDefinition Width="38px"></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Label Margin="0,0,0,0" Grid.Column="0" HorizontalAlignment="Right" Content="{Binding SizeName}" FontSize="17" Foreground="#666666"/>
                                                                <Label Grid.Column="1" HorizontalAlignment="Left" Content="-" FontSize="17" Foreground="#666666" />
                                                                <Label Grid.Column="2" HorizontalAlignment="Left" Content="{Binding SizeCount}" FontSize="17" Foreground="#2167AF"/>
                                                            </Grid>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </ListBox.ItemTemplate>
                                                <ListBox.ItemContainerStyle>
                                                    <Style TargetType="ListBoxItem">
                                                        <!-- 设置控件模板 -->
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="ListBoxItem">
                                                                    <Border Background="{TemplateBinding Background}">
                                                                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                                                    </Border>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </ListBox.ItemContainerStyle>
                                            </ListBox>
                                        </Grid>
                                    </Grid>
                                    <Grid Visibility="{Binding Path=Visible2}" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <StackPanel Grid.Row="0" Width="130px" Height="42px" Background="#2167AF">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Label Grid.Column="0" Content="{Binding ClassName}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Foreground="#FFFFFF" ></Label>
                                                <Label Grid.Column="1" Content="{Binding ClassCount}" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="#FFFFFF" ></Label>
                                            </Grid>
                                        </StackPanel>
                                        <StackPanel  Grid.Row="1" Width="130px" Height="98px" Background="#FFFFFF">
                                            <Label Content="均码" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,30,0,0" FontSize="22" Foreground="#666666"></Label>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </StackPanel>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="ListBoxItem">
                            <!-- 设置控件模板 -->
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border Background="{TemplateBinding Background}">
                                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                 VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                 TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>
            </StackPanel>

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="30*"></RowDefinition>
                    <RowDefinition Height="11*"></RowDefinition>
                </Grid.RowDefinitions>
                <TextBox Width="0" Name="txtCard" Text="{Binding Model.ICCode,UpdateSourceTrigger=PropertyChanged}"></TextBox>
                <Button Width="0" IsDefault="True" Command="{Binding CardCommand}"></Button>
                <StackPanel Width="200px" Height="200px" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,34,26,0">
                    <Border Width="200px" Height="200px"  Opacity="0.7" Background="#21AF37" CornerRadius="10" Margin="0,0,0,0">
                        <Button Command="{Binding SelectVal}">
                            <Button.Template>
                                <ControlTemplate>
                                    <StackPanel>
                                        <Label Margin="0,15,0,0" FontSize="22" Foreground="#FFFFFF" Content="请刷卡/指纹领取" FontFamily="微软雅黑" HorizontalAlignment="Center"></Label>
                                        <Grid Height="150">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Image  Source="{StaticResource zhiwen}" Width="64" Height="64" Margin="18,39,18,47"></Image>
                                            <Image Grid.Column="1"  Source="{StaticResource hand}" Width="68" Height="95" Margin="16,24,16,31"></Image>
                                        </Grid>
                                    </StackPanel>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>
                    </Border>
                </StackPanel>
                <StackPanel Grid.Row="1" HorizontalAlignment="Right" Width="200" Margin="0,8,26,0">
                    <Border Width="200px" Height="80px" Background="#292E42" CornerRadius="10" Opacity="0.7">
                        <StackPanel>
                            <Grid>
                                <Label Content="刷卡投放" FontSize="22px" Foreground="#FFFFFF" Margin="80,15,0,0"/>
                                <Image Source="{StaticResource ResourceKey=toudi}" Width="43" Height="64" Margin="0,5,90,0"/>
                            </Grid>
                        </StackPanel>
                    </Border>
                </StackPanel>
            </Grid>
        </Grid>
        <Canvas>
            <Button Command="{Binding Close}" Content="关门" FontSize="19px" Canvas.Left="604" Canvas.Top="302"></Button>
        </Canvas>
        <Canvas>
            <Button Command="{Binding Open}" Content="开门" FontFamily="19px" FontSize="19px" Canvas.Left="706" Canvas.Top="302"></Button>
        </Canvas>
        <Canvas>
            <Line X1="200" Y1="10" X2="200" Y2="100" Stroke="#FFFFFF"  StrokeThickness="1" Canvas.Left="474" Canvas.Top="345"></Line>
        </Canvas>
    </Grid>
</Window>
