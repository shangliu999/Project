.conditionTitle {
    font-family: 'Microsoft YaHei';
    font-size: 14px;
    color: #333;
}

.lefttxt {
    text-align: left;
    padding-left: 10px;
    border: 1px solid #CCC;
}

.righttxt {
    text-align: right;
    padding-right: 10px;
    border: 1px solid #CCC;
}
/*偶数行*/
.eventr {
    height: 34px;
    background-color: #FBFBFB;
}

    .eventr:hover {
        border-radius: 4px;
        background-color: #0294DD;
        color: #FFF;
        cursor: pointer;
    }
/*奇数行*/
.oddtr {
    height: 34px;
    background-color: #F1F1F1;
}

    .oddtr:hover {
        border-radius: 4px;
        background-color: #0294DD;
        color: #FFF;
        cursor: pointer;
    }
