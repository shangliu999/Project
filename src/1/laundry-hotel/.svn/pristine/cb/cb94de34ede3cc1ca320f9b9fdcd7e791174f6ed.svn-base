<div class="modal inmodal" id="myModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">外包收货</h4>
            </div>
            <form class="form-horizontal">
                <div class="modal-body">
                    <div class="form-group">
                        <label class="col-lg-2 control-label">酒店</label>
                        <div class="col-lg-4">
                            <label id="lblHotelName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">状态</label>
                        <div class="col-lg-4">
                            <label id="lblStateName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">流水号</label>
                        <div class="col-lg-4">
                            <label id="lblNo" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">类型</label>
                        <div class="col-lg-4">
                            <label id="lblSubTypeName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">收货时间</label>
                        <div class="col-lg-4">
                            <label id="lblCreateTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">收货人</label>
                        <div class="col-lg-4">
                            <label id="lblCreateName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div><div class="form-group">
                        <label class="col-lg-2 control-label">备注</label>
                        <div class="col-lg-10">
                            <label id="lblNote" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="table-responsive dataTables_wrapper" style="margin-left:20px;margin-right:20px;">
                            <table id="textile-table-detail" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>纺织品名称</th>
                                        <th>尺寸</th>
                                        <th>数量</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div>
    </div>
</div>

