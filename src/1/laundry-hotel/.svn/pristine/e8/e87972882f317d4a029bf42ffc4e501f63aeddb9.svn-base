
@{
    Layout = null;
}
<div class="modal inmodal" id="MaintainModel" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="DetailTitel"></h4>
            </div>
            <form id="maintain" class="form-horizontal">
                <div class="modal-body">
                    <div class="form-group">
                        <label class="col-lg-2 control-label">单据号</label>
                        <div class="col-lg-4">
                            <label id="InvNo" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">状态</label>
                        <div class="col-lg-4">
                            <label id="Star" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">收货人</label>
                        <div class="col-lg-4">
                            <label id="CreatUser" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">收货时间</label>
                        <div class="col-lg-4">
                            <label id="CreatTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">酒店</label>
                        <div class="col-lg-4">
                            <select id="HotelName" class="control-label hotel" style="font-weight:normal;">
                                @{List<ETexsys.Model.region> regionlist = ViewData["regionlist"] as List<ETexsys.Model.region>;}
                                @foreach (var item in regionlist)
                                {
                                    <option value="@item.ID" >@item.RegionName</option>
                                }
                            </select>
                        </div>
                        <label class="col-lg-2 control-label">区域</label>
                        <div class="col-lg-4">
                            <select  id="FloorName" class="control-label" style="font-weight:normal;"></select>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="table-responsive dataTables_wrapper" style="margin-left:20px;margin-right:20px;">
                            <table id="maintain-table-detail" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>名称</th>
                                        <th>尺寸</th>
                                        <th>数量</th>
                                        <th>异常数</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                    <button type="submit" class="btn btn-primary">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>