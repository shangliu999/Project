<div class="modal inmodal" id="myModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">商城订单</h4>
            </div>
            <form class="form-horizontal">
                <div class="modal-body">
                    <div class="form-group">
                        <input id="hidOrderID" value="0" type="hidden" />
                        <label class="col-lg-2 control-label">流水号</label>
                        <div class="col-lg-4">
                            <label id="lblOrderNo" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">酒店</label>
                        <div class="col-lg-4">
                            <label id="lblHotelName" class="control-label" style="font-weight:normal;"></label>
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">订单状态</label>
                        <div class="col-lg-4">
                            <label id="lblStateName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">支付状态</label>
                        <div class="col-lg-4">
                            <label id="lblPaymentTypeName" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">总金额</label>
                        <div class="col-lg-4">
                            <label id="lblGoodsTotal" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">总数量</label>
                        <div class="col-lg-4">
                            <label id="lblGoodsTotalPrice" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">下单时间</label>
                        <div class="col-lg-4">
                            <label id="lblCreateTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">期望时间</label>
                        <div class="col-lg-4">
                            <label id="lblDelivery" class="control-label" style="font-weight:normal;"></label>
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">确认时间</label>
                        <div class="col-lg-4">
                            <label id="lblComfirmTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">配货时间</label>
                        <div class="col-lg-4">
                            <label id="lblDistributionTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-2 control-label">送达时间</label>
                        <div class="col-lg-4">
                            <label id="lblSignTime" class="control-label" style="font-weight:normal;"></label>
                        </div>
                        <label class="col-lg-2 control-label">配送人员</label>
                        <div class="col-lg-4">
                            <label id="lblDistributor" class="control-label" style="font-weight:normal;"></label>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="table-responsive dataTables_wrapper" style="margin-left:20px;margin-right:20px;">
                            <table id="order-table-detail" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>商品</th>
                                        <th style="text-align:right;padding-right:5px;">数量</th>
                                        <th style="text-align:right;padding-right:5px;">金额</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="button" id="btnSubmit" class="btn btn-primary" value="确认订单" />
                    <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                </div>
            </form>
        </div>
    </div>
</div>


